{
	"plugins": ["transform-react-jsx"],
  "presets": ["es2015", "stage-0", "react"],
  "env": {
    "development": {
      "plugins": [
        "typecheck",
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react", "redbox-react"],
            "locals": ["module"]
          }
        ]
      }]]
    },
    "testing": {
      "plugins": []
    }
  }
}
